{
    "name": "Rapid",
    "version": "0.1",
    "description": "Chrome DevTools Extension for Rapid",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        ["script.js"]
    ],
    "manifest_version": 2
}